{
  "name": "lambda-rollbar",
  "version": "0.0.0-development",
  "description": "Rollbar wrapper for Serverless, API Gateway and Lambda.",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "npx babel src --out-dir lib --source-maps inline --copy-files --include-dotfiles",
    "build-clean": "npm run clean && npm run build",
    "test-simple": "nyc mocha \"./test/**/*.spec.js\"",
    "test": "npm run clean && npm run gardener && npm run test-simple",
    "coveralls": "node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
    "semantic-release": "npm run build-clean && npx semantic-release",
    "gardener": "node gardener"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blackflux/lambda-rollbar.git"
  },
  "keywords": [
    "rollbar",
    "lambda",
    "serverless",
    "wrapper",
    "api"
  ],
  "author": "Lukas Siemon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blackflux/lambda-rollbar/issues"
  },
  "homepage": "https://github.com/blackflux/lambda-rollbar#readme",
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-object-rest-spread": "7.4.3",
    "@babel/register": "7.4.0",
    "@blackflux/eslint-plugin-rules": "1.0.2",
    "@blackflux/robo-config-plugin": "1.4.2",
    "babel-eslint": "10.0.1",
    "chai": "4.2.0",
    "coveralls": "3.0.3",
    "eslint": "5.16.0",
    "js-gardener": "2.0.4",
    "nyc": "13.3.0",
    "semantic-release": "15.13.3"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/blackflux/lambda-rollbar/blob/master/LICENSE"
    }
  ],
  "engines": {
    "node": ">= 6.10.3"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "fast-safe-stringify": "2.0.6",
    "lodash.get": "4.4.2",
    "lodash.omit": "4.5.0",
    "rollbar": "2.5.5"
  },
  "peerDependencies": {},
  "nyc": {
    "tempDir": "./coverage/.nyc_output",
    "report-dir": "./coverage",
    "check-coverage": true,
    "per-file": false,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "gardener.js",
      "node_modules/*",
      "coverage/*",
      "lib/*"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "@babel/register"
    ],
    "extension": [],
    "cache": true,
    "all": true,
    "babel": true
  }
}
